{
  "name": "@rlhawk/eleventy-starter",
  "version": "1.0.0",
  "description": "An Eleventy starter project, built with Tailwind, Alpine, and Storybook.",
  "keywords": [],
  "license": "GPL-2.0",
  "scripts": {
    "start": "eleventy --serve & postcss src/style.css --output tmp/style.css --watch",
    "build": "cross-env ELEVENTY_PRODUCTION=true eleventy && NODE_ENV=production postcss src/style.css --output build/css/style.css",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@babel/core": "^7.12.10",
    "@storybook/addon-essentials": "^6.2.1",
    "@storybook/addon-links": "^6.2.1",
    "@storybook/html": "^6.2.1",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/typography": "^0.4.0",
    "alpinejs": "^2.8.2",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "html-minifier": "^4.0.0",
    "nunjucks": "^3.2.2",
    "nunjucks-loader": "^3.0.0",
    "postcss": "^8.2.2",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.0.4",
    "tailwindcss-debug-screens": "^2.0.0"
  },
  "dependencies": {}
}
